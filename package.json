{
    "name": "angular-radagast",
    "version": "0.0.1",
    "description": "wizard component for angular",
    "license": "MIT",
    "author": "Brittany Moore <brittanymoore>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/angular-radagast.git"
    },
    "scripts": {
        "build": "webpack --env=dev",
        "build:prod": "npm run clean && webpack --env=prod",
        "clean": "rimraf ./dist",
        "lint": "tslint -c ./config/tslint.json --type-check 'src/**/*.ts' -p ./tsconfig.json",
        "start": "webpack-dev-server --env=dev --open --progress",
        "start:prod": "npm run clean && webpack-dev-server --env=prod --open --progress",
        "test": "karma start ./config/karma.conf.js ",
        "test:e2e": "protractor ./config/protractor.conf.js",
        "webdriver-manager": "webdriver-manager update --standalone false --gecko false"
    },
    "dependencies": {
        "@angular/animations": "4.0.1",
        "@angular/common": "4.0.1",
        "@angular/compiler": "4.0.1",
        "@angular/compiler-cli": "4.0.1",
        "@angular/core": "4.0.1",
        "@angular/http": "4.0.1",
        "@angular/platform-browser": "4.0.1",
        "@angular/platform-browser-dynamic": "4.0.1",
        "@angular/platform-server": "4.0.1",
        "@angular/router": "4.0.1",
        "core-js": "2.4.1",
        "ie-shim": "0.1.0",
        "rxjs": "5.0.1",
        "zone.js": "0.8.5"
    },
    "devDependencies": {
        "@angular/cli": "^1.4.8",
        "@ngtools/webpack": "1.3.1",
        "@types/core-js": "0.9.39",
        "@types/jasmine": "2.5.46",
        "@types/node": "7.0.11",
        "angular-router-loader": "0.6.0",
        "angular2-template-loader": "0.6.0",
        "autoprefixer": "7.1.0",
        "awesome-typescript-loader": "3.1.2",
        "codelyzer": "3.0.1",
        "css-loader": "0.27.3",
        "exports-loader": "0.6.3",
        "html-webpack-plugin": "2.24.1",
        "jasmine": "2.5.3",
        "karma": "1.2.0",
        "karma-chrome-launcher": "2.0.0",
        "karma-jasmine": "1.0.2",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "1.8.0",
        "node-sass": "4.5.3",
        "postcss-loader": "2.0.5",
        "protractor": "5.1.1",
        "raw-loader": "0.5.1",
        "sass-loader": "6.0.5",
        "ts-node": "1.7.0",
        "tslint": "5.3.0",
        "typescript": "2.2.2",
        "webpack": "2.3.2",
        "webpack-dev-server": "2.4.2",
        "webpack-merge": "4.1.0"
    }
}
